<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Flow: list animals -->
	<bean id="listCarrosValidate"  class="br.com.grupomult.flows.carros.ListCarrosValidate"  />
	<bean id="listCarrosLoad"      class="br.com.grupomult.flows.carros.ListCarrosLoad"      />
	<bean id="listCarrosConverter" class="br.com.grupomult.flows.carros.ListCarrosConverter" />
	<bean id="listCarrosResponse"  class="br.com.grupomult.flows.carros.ListCarrosResponse"  />

	<!-- Flow: list animals by id -->
	<bean id="listCarrosByIdValidate"     class="br.com.grupomult.flows.carros.ListCarrosByIdValidate"     />
	<bean id="listCarrosByIdLoad"         class="br.com.grupomult.flows.carros.ListCarrosByIdLoad"         />
	<bean id="listCarrosByIdLoadValidate" class="br.com.grupomult.flows.carros.ListCarrosByIdLoadValidate" />
	<bean id="listCarrosByIdConverter"    class="br.com.grupomult.flows.carros.ListCarrosByIdConverter"    />
	<bean id="listCarrosByIdResponse"     class="br.com.grupomult.flows.carros.ListCarrosByIdResponse"     />

</beans>